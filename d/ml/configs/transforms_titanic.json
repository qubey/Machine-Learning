{
  "output": {
    "features": false,
    "target": false,
    "delimiter": ","
  },
  "model": {
    "type": "naive_bayes",
    "k": 2,
    "feature_prior": 1.0,
    "feature_prior_weight": 1,
    "class_prior_weight": 1
  },
  "transforms": [
    {
      "name": "age_norm",
      "type": "normalize",
      "input": [ "age" ],
      "output": false
    },
    {
      "name": "fare_norm",
      "type": "normalize",
      "input": [ "fare" ],
      "output": false
    },
    {
      "name": "fare_qb",
      "type": "quantile_bucket",
      "buckets": 10,
      "output": true,
      "input": [ "fare" ]
    },
    {
      "name": "name_word",
      "type": "words",
      "max_card": 3,
      "output": true,
      "input": [ "name" ]
    },
    {
      "name": "name_age_ngram",
      "type": "ngram",
      "output": true,
      "input": [ "age_qb", "name_word" ]
    },
    {
      "name": "name_age_cat",
      "type": "vec_to_id",
      "output": false,
      "input": [ "name_age_ngram" ]
    },
    {
      "name": "age_norm_gt",
      "type": "greater_than",
      "input": [ "age_norm" ],
      "output": false,
      "threshold": 0.2
    },
    {
      "name": "age_qb",
      "type": "quantile_bucket",
      "input": [ "age" ],
      "buckets": 10,
      "output": true
    }
  ]
}
