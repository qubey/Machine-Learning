{
  "model": {
    "type": "kmedoids",
    "k": 2,
    "seed": 123,
    "iterations": 4
  },
  "transforms": [
    {
      "name": "age_norm",
      "type": "normalize",
      "input": [ "age" ],
      "output": true
    },
    {
      "name": "fare_norm",
      "type": "normalize",
      "input": [ "fare" ],
      "output": true
    },
    {
      "name": "fare_qb",
      "type": "quantile_bucket",
      "buckets": 10,
      "output": false,
      "input": [ "fare" ]
    },
    {
      "name": "name_word",
      "type": "words",
      "max_card": 3,
      "output": false,
      "input": [ "name" ]
    },
    {
      "name": "name_age_ngram",
      "type": "ngram",
      "output": false,
      "input": [ "age_qb", "name_word" ]
    },
    {
      "name": "name_age_cat",
      "type": "vec_to_id",
      "output": false,
      "input": [ "name_age_ngram" ]
    },
    {
      "name": "age_norm_gt",
      "type": "greater_than",
      "input": [ "age_norm" ],
      "output": false,
      "threshold": 0.2
    },
    {
      "name": "age_qb",
      "type": "quantile_bucket",
      "input": [ "age" ],
      "buckets": 15,
      "output": false
    }
  ]
}
